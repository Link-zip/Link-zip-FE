<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Widget.BottomNavigationView"
        parent="Widget.Design.BottomNavigationView">
        <item name="fontFamily">@font/pretendard_light</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textSize">12sp</item>
    </style>
    <style name="item_list_ziptitle">
        <item name="fontFamily">@font/pretendard_regular</item>
        <item name="android:textSize">12sp</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:textColor">@color/nav_selected</item>
