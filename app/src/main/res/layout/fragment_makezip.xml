<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@color/white">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:focusable="false"
        tools:context=".presentation.MainActivity">



        <TextView
            android:id="@+id/makezip_msg1"
            android:layout_width="match_parent"
            android:layout_height="56dp"
            android:textAlignment="center"
            android:background="@color/light_gray"
            app:layout_constraintTop_toTopOf="parent"
            />

        <ImageView
            android:id="@+id/Sumnailzip"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="33dp"
            android:src="@drawable/thumb_makezip_thumbmakezip"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/makezip_msg1"
            app:layout_constraintBottom_toTopOf="idzip"/>

        <LinearLayout
            android:id="@+id/makezip_option"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/Sumnailzip"
            android:orientation="vertical">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/zip_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@id/Sumnailzip">

            <View
                android:id="@+id/makezip_msg3"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="parent"
                app:layout_constraintWidth_percent="0.0888"
                tools:ignore="MissingConstraints" />


            <TextView
                android:id="@+id/zip_name_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/pretendard_regular"
                android:text="Zip 이름"
                android:textColor="@color/black"
                android:textFontWeight="600"
                android:textSize="16dp"
                app:layout_constraintStart_toEndOf="@id/makezip_msg3"
                tools:layout_editor_absoluteY="0dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0/30"
                android:textSize="14dp"
                android:fontFamily="@font/pretendard_light"
                android:textColor="#666666"
                app:layout_constraintEnd_toStartOf="@id/makezip_msg4"
                android:textFontWeight="400" />

            <View
                android:id="@+id/makezip_msg4"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintDimensionRatio="1:1"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="parent"
                app:layout_constraintWidth_percent="0.0888"
                app:layout_constraintEnd_toEndOf="parent"
                tools:ignore="MissingConstraints" />


        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/zip_name_enter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="15dp"
            android:paddingLeft="32dp"
            android:paddingRight="32dp"
            app:layout_constraintTop_toBottomOf="@id/zip_name">

            <TextView
                android:id="@+id/zip_name_enter_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Zip 이름을 입력해주세요."
                android:textSize="16dp"
                android:fontFamily="@font/pretendard_regular"
                android:textColor="#666666"
                android:textFontWeight="400"
                tools:ignore="MissingConstraints" />

            <View
                android:id="@+id/line"
                android:layout_width="312dp"
                android:layout_height="2px"
                android:layout_marginTop="12dp"
                android:background="@color/black"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/zip_name_enter_tv" />
        </androidx.constraintlayout.widget.ConstraintLayout>

            <LinearLayout
                android:id="@+id/colors"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="16dp"
                android:paddingLeft="32dp"
                android:paddingRight="32dp"
                app:layout_constraintTop_toBottomOf="@id/line">

                <View
                    android:id="@+id/rectangle_1"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="3dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/rectangle_1"
                    />

                <View
                    android:id="@+id/rectangle_2"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/rectangle_2"/>

                <View
                    android:id="@+id/rectangle_3"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/rectangle_3"/>

                <View
                    android:id="@+id/rectangle_4"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/rectangle_4"/>

                <View
                    android:id="@+id/rectangle_5"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/rectangle_5"/>

                <View
                    android:id="@+id/rectangle_6"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/rectangle_6"/>

                <View
                    android:id="@+id/rectangle_7"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginRight="10dp"
                    android:background="@drawable/rectangle_7"/>
            </LinearLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <View
                    android:id="@+id/makezip_msg5"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toTopOf="@+id/select_pic_IV"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintWidth_percent="0.0259"
                    tools:ignore="MissingConstraints" />

                <ImageView
                    android:id="@+id/select_pic_IV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/btn_makezip_selectpic"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toTopOf="@id/makezip_msg6"
                    android:layout_marginTop="16dp"/>

                <View
                    android:id="@+id/makezip_msg6"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/select_pic_IV"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/select_pic_IV"
                    app:layout_constraintWidth_percent="0.3"
                    tools:ignore="MissingConstraints" />

                <ImageView
                    android:id="@+id/select_pic_finish_IV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/drawerbtn_finish"
                    app:layout_constraintBottom_toTopOf="@id/makezip_msg7"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/makezip_msg6"
                    app:layout_constraintVertical_bias="1.0" />

                <View
                    android:id="@+id/makezip_msg7"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/select_pic_finish_IV"
                    app:layout_constraintDimensionRatio="1:1"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintWidth_percent="0.0389"
                    tools:ignore="MissingConstraints" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </LinearLayout>

        <androidx.fragment.app.FragmentContainerView
            android:id="@+id/nav_host_fragment"
            android:name="androidx.navigation.fragment.NavHostFragment"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:defaultNavHost="true"
            app:layout_constraintTop_toTopOf="parent"
            app:navGraph="@navigation/main_graph" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
