<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/iv_item_list_img_main"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:layout_marginEnd="255dp"
        android:background="@drawable/shadow_item_list"
        android:scaleType="fitCenter"
        android:src="@drawable/item_list_zip_example"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintWidth_percent="0.305" />

    <ImageView
        android:id="@+id/iv_item_list_type_text"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="52:28"
        app:layout_constraintWidth_percent="0.144"
        android:src="@drawable/ic_link_textlink"
        android:layout_marginStart="0dp"
        android:scaleType="fitCenter"
        app:layout_constraintStart_toStartOf="@id/iv_item_list_img_main"
        app:layout_constraintBottom_toBottomOf="@id/iv_item_list_img_main"
        />

    <ImageView
        android:id="@+id/iv_item_list_type_link"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="40:28"
        app:layout_constraintWidth_percent="0.11"
        android:src="@drawable/ic_link_link"
        android:layout_marginStart="0dp"
        android:scaleType="fitCenter"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="@id/iv_item_list_img_main"
        app:layout_constraintBottom_toBottomOf="@id/iv_item_list_img_main"
        />

    <ImageView
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="24:20.66"
        app:layout_constraintWidth_percent="0.0667"
        android:src="@drawable/ic_heart_selected"
        app:layout_constraintEnd_toEndOf="@id/iv_item_list_img_main"
        app:layout_constraintBottom_toBottomOf="@id/iv_item_list_img_main"
        android:layout_marginEnd="7dp"
        android:layout_marginBottom="6dp"
        />


    <ImageView
        android:id="@+id/iv_item_list_zip"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintDimensionRatio="27.14:21.04"
        app:layout_constraintWidth_percent="0.0604"
        android:src="@drawable/ic_item_zip_yellow"
        android:layout_marginStart="18dp"
        android:layout_marginTop="3dp"
        app:layout_constraintStart_toEndOf="@id/iv_item_list_img_main"
        app:layout_constraintTop_toTopOf="@id/iv_item_list_img_main"
        />

    <TextView
        android:id="@+id/tv_item_list_zip_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:layout_marginBottom="4dp"
        android:layout_marginStart="8.26dp"
        android:text="인사이트"
        android:textAppearance="@style/item_list_ziptitle"
        android:lineSpacingExtra="2sp"
        app:layout_constraintStart_toEndOf="@+id/iv_item_list_zip"
        app:layout_constraintTop_toTopOf="@+id/iv_item_list_zip"
        app:layout_constraintBottom_toBottomOf="@id/iv_item_list_zip"/>

    <TextView
        android:id="@+id/tv_item_list_link_name"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="-2dp"
        android:gravity="center_vertical"
        android:lineSpacingExtra="5sp"
        android:text="마이크로/나노 인플루언서 마케팅 전략 ## 최대 2줄"
        android:textAppearance="@style/item_list_linktitle"
        app:layout_constraintBottom_toTopOf="@id/tv_item_list_link_date"
        app:layout_constraintStart_toStartOf="@id/iv_item_list_zip"
        app:layout_constraintTop_toBottomOf="@id/iv_item_list_zip"
        app:layout_constraintWidth_percent="0.516" />

    <TextView
        android:id="@+id/tv_item_list_link_date"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintWidth_percent="0.516"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="3dp"
        android:textAppearance="@style/item_list_linkdate"
        android:lineSpacingExtra="2sp"
        android:text="2024.06.11"
        app:layout_constraintStart_toStartOf="@id/tv_item_list_link_name"
        app:layout_constraintBottom_toBottomOf="@id/iv_item_list_img_main" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/nav_unselected"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/iv_item_list_img_main"
        />
</androidx.constraintlayout.widget.ConstraintLayout>
