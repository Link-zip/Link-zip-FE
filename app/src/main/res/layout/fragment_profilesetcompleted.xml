<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <ImageView
        android:id="@+id/iv_profilesetcompleted_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_gnb_back"
        android:layout_marginTop="54dp"
        android:layout_marginStart="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_profilesetcompleted_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/pretendard_semibold"
        android:lineHeight="28sp"
        android:textSize="20sp"
        android:text="프로필 설정"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/iv_profilesetcompleted_back"
        app:layout_constraintBottom_toBottomOf="@id/iv_profilesetcompleted_back" />

    <ImageView
        android:id="@+id/iv_profilesetcompleted_3dhand"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/img_profilesetcompleted_3dhand"
        android:layout_marginTop="36dp"
        app:layout_constraintTop_toBottomOf="@id/tv_profilesetcompleted_title"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/tv_profilesetcompleted_complete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="프로필 설정이\n완료됐어요!"
        android:gravity="center"
        android:fontFamily="@font/pretendard_semibold"
        android:textSize="30sp"
        android:textColor="@color/black"
        android:lineHeight="42sp"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@id/iv_profilesetcompleted_3dhand"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/tv_profilesetcompleted_welcome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="뉴비 님, Link.zip에\n오신 걸 환영해요!"
        android:gravity="center"
        android:fontFamily="@font/pretendard_regular"
        android:textSize="16sp"
        android:textColor="@color/black"
        android:lineHeight="24sp"
        android:letterSpacing="-0.025"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@id/tv_profilesetcompleted_complete"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_profilesetcompleted_start"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_rect_8_1191ad_fill"
        android:layout_marginHorizontal="24dp"
        android:text="시작하기"
        android:fontFamily="@font/pretendard_semibold"
        android:textSize="18sp"
        android:lineHeight="25sp"
        android:letterSpacing="-0.025"
        android:textColor="@color/white"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>